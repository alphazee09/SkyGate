# SkyGate Integration Configuration

# Environment variables for development
REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_ENV=development

# Authentication settings
REACT_APP_TOKEN_EXPIRY=3600
REACT_APP_REFRESH_TOKEN_EXPIRY=604800

# Upload settings
REACT_APP_MAX_UPLOAD_SIZE=50000000
REACT_APP_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,image/bmp,image/webp,image/tiff,video/mp4,video/mov,video/avi,video/webm

# Feature flags
REACT_APP_ENABLE_VIDEO_DETECTION=true
REACT_APP_ENABLE_BATCH_UPLOAD=false
REACT_APP_ENABLE_SOCIAL_SHARING=true
